{
  "extends": "@electron-toolkit/tsconfig/tsconfig.web.json",
  "include": [
    "src/**/*.ts",
    "src/**/*.tsx",
    "src/preload/*.d.ts"
  ],
  "exclude": [
    "src/main/**/*",
    "src/preload/index.ts",
    "src/renderer/**/*"
  ],
  "compilerOptions": {
    "composite": true,
    "jsx": "react-jsx",
    "baseUrl": "."
  }
}
